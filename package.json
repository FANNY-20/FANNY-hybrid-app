{
  "name": "fanny-hybrid-app",
  "version": "0.1.2",
  "author": "Benoit Fleuriaud <benoit.fleuriaud@soyhuce.fr>",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt",
    "build": "nuxt build",
    "build:native": "yarn build && yarn cap copy && yarn cap open",
    "start": "cross-env NODE_ENV=production nuxt start",
    "generate": "nuxt generate",
    "lint": "yarn eslint && yarn stylelint",
    "lint:check": "yarn eslint:check && yarn stylelint:check",
    "eslint": "yarn eslint:check --fix",
    "eslint:check": "eslint --format node_modules/eslint-friendly-formatter --ext .js,.vue --ignore-path .gitignore .",
    "stylelint": "yarn stylelint:check --fix",
    "stylelint:check": "stylelint --config stylelint.config.js './**/*.{vue,scss,css}'",
    "prettify": "prettier --config prettier.config.js --ignore-path .gitignore --write './**/*.{tpl,md,yml,json}'",
    "prettify:check": "prettier --config prettier.config.js --ignore-path .gitignore --check './**/*.{tpl,md,yml,json}'",
    "e2e": "node test/e2e/runner.js --env chrome-headless,firefox-headless",
    "unit": "mocha --config mocha.config.js './**/*.test.js'",
    "test": "yarn unit && yarn e2e",
    "postinstall": "jetifier",
    "resources": " yarn cordova-res ios --skip-config --copy && yarn cordova-res android --skip-config --copy"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "@capacitor/cli": "^2.0.1",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/style-resources": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "chromedriver": "^81.0.0",
    "cordova-res": "^0.15.0",
    "cross-env": "^7.0.2",
    "cross-spawn": "^7.0.2",
    "eslint": "^7.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "^6.2.2",
    "express": "^4.17.1",
    "geckodriver": "^1.19.1",
    "html-loader": "^1.1.0",
    "jetifier": "^1.6.5",
    "mocha": "^7.1.2",
    "nightwatch": "^1.3.4",
    "node-sass": "^7.0.0",
    "postcss-em-media-query": "^2.0.0",
    "postcss-pxtorem": "^5.1.1",
    "prettier": "1.17.1",
    "sass-loader": "^8.0.2",
    "selenium-server-standalone-jar": "^3.141.59",
    "stylelint": "^13.3.3"
  },
  "dependencies": {
    "@capacitor/android": "2.1.0",
    "@capacitor/core": "^2.0.1",
    "@capacitor/ios": "2.1.0",
    "@hapi/joi": "^17.1.1",
    "@ionic-native/background-geolocation": "^5.24.0",
    "@ionic-native/core": "^5.24.0",
    "@mauron85/cordova-plugin-background-geolocation": "^3.1.0",
    "@nuxtjs/axios": "^5.10.2",
    "@nuxtjs/vuetify": "^1.11.2",
    "js-sha256": "^0.9.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.forown": "^4.4.0",
    "lodash.intersection": "^4.4.0",
    "lodash.isplainobject": "^4.0.6",
    "nuxt": "2.12.2",
    "peerjs": "^1.2.0",
    "svg-parser": "^2.0.4",
    "uuid": "^8.0.0",
    "vuetify-toast-snackbar": "^0.6.1"
  }
}
